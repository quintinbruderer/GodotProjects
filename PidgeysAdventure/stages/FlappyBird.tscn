resource_name = "button_instructions"
script/source = "#(built in) button instructions

extends TextureButton

var button_tap
var button_get_ready


func _ready():
	button_tap = utils.get_main_node().get_node('HUD/button_instructions/tap')
	button_get_ready = utils.get_main_node().get_node('HUD/button_instructions/get_ready')
	button_get_ready.hide()
	connect('pressed', self, '_on_pressed')
	pass

func _on_pressed():
	button_tap.hide()
	button_get_ready.show()
	
	var bird = utils.get_main_node().get_node('pidgey')
	if bird:
		bird.set_new_state(bird.flap_num)
	
	pass
